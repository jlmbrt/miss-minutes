{
    "name": "miss-minutes",
    "version": "0.0.3",
    "description": "How long does my code take?",
    "main": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "types:check": "tsc --noEmit",
        "types:watch": "npm run types:check -- --watch",
        "build": "tsc ",
        "build:full": "rimraf dist/* && npm run build -- --incremental false",
        "format:check": "prettier --check ./src",
        "format:fix": "prettier --write ./src",
        "lint": "eslint ./src",
        "test": "jest .",
        "test:watch": "npm run test -- --watch",
        "prepublishOnly": "npm run check-all && npm run build:full",
        "check-all": "npm run lint && npm run format:check && npm run types:check && npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jlmbrt/miss-minutes.git"
    },
    "keywords": [],
    "author": "Julien Lambert",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jlmbrt/miss-minutes/issues"
    },
    "homepage": "https://github.com/jlmbrt/miss-minutes#readme",
    "devDependencies": {
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.14",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "jest": "^27.4.7",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.1.3",
        "typescript": "^4.5.5"
    }
}
